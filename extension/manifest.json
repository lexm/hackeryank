{
  "manifest_version": 2,

  "name": "HackerYank",
  "description": "Saves one's HackerRank solutions for adding to a repo",
  "version": "1.5",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://www.hackerrank.com/"
  ]
}
